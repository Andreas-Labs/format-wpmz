{
  "name": "@format/wpml-kml",
  "version": "0.0.3",
  "license": "MIT",
  "exports": "./src/mod.ts",
  "tasks": {
    "dev": "deno run --watch --allow-read --allow-write ./src/main.ts",
    "test": "deno test --allow-read --allow-write --parallel"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1"
  },
  "exclude": [
    ".github",
    ".vscode",
    "bin"
  ],
  "fmt": {
    "useTabs": true,
    "lineWidth": 120,
    "indentWidth": 4,
    "semiColons": true,
    "singleQuote": true,
    "include": ["src/"]
  }
}
